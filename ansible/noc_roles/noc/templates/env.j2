# {{ ansible_managed }}
export NOC_CONFIG="{{ config_order }}"
export PGPASSWORD="{{ postgres_noc_password }}"
export NOC_USER="{{ noc_user }}"
export PGDATABASE="{{ postgres_noc_db }}"
export MONGO_DIR="{{ mongo_dir }}"
export PYTHONPATH="{{ noc_root | dirname }}"
export DJANGO_SETTINGS_MODULE="noc.settings"
{% if custom_path is defined %}
export CUSTOM_PATH="{{ custom_path }}"
{% endif %}
