{
  "name": "noc-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "nx serve --proxy-config proxy.conf.json --port 4200 --configuration=en",
    "start:ru": "nx serve --proxy-config proxy.conf.json --port 4201 --configuration=ru",
    "build": "nx build --with-deps --prod",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "preextract-i18n": "ngc -p apps/desktop/tsconfig.app.json",
    "extract-i18n": "localize-extract -s \"dist/out-tsc/**/*.js\" --useLegacyIds false -f xlf -o dist/locale/messages.xlf && ng run desktop:xliffmerge",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.0",
    "@angular/common": "^10.2.0",
    "@angular/compiler": "^10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/forms": "^10.2.0",
    "@angular/platform-browser": "^10.2.0",
    "@angular/platform-browser-dynamic": "^10.2.0",
    "@angular/router": "^10.2.0",
    "@ngrx/effects": "10.0.1",
    "@ngrx/entity": "10.0.1",
    "@ngrx/router-store": "10.0.1",
    "@ngrx/store": "10.0.1",
    "@nrwl/angular": "10.3.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "^10.2.0",
    "@angular/language-service": "^10.2.0",
    "@angular/localize": "10.2.0",
    "@ngrx/schematics": "10.0.1",
    "@ngrx/store-devtools": "10.0.1",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@nrwl/cli": "10.3.1",
    "@nrwl/cypress": "10.3.1",
    "@nrwl/jest": "10.3.1",
    "@nrwl/workspace": "10.3.1",
    "@types/jest": "26.0.15",
    "@types/node": "~14.14.5",
    "codelyzer": "~6.0.1",
    "cypress": "^5.5.0",
    "dotenv": "8.2.0",
    "eslint": "7.12.1",
    "jest": "26.6.1",
    "jest-preset-angular": "8.3.2",
    "ng-packagr": "^10.1.0",
    "prettier": "2.1.2",
    "ts-jest": "26.4.3",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5"
  }
}
