{
  "version": 1,
  "projects": {
    "desktop": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/desktop",
      "sourceRoot": "apps/desktop/src",
      "prefix": "noc",
      "i18n": {
        "sourceLocale": "en",
        "locales": {
          "ru": "apps/desktop/src/locale/messages.ru.xlf"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/desktop",
            "index": "apps/desktop/src/index.html",
            "main": "apps/desktop/src/main.ts",
            "polyfills": "apps/desktop/src/polyfills.ts",
            "tsConfig": "apps/desktop/tsconfig.app.json",
            "aot": true,
            "localize": true,
            "assets": [
              "apps/desktop/src/favicon.ico",
              "apps/desktop/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/desktop/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/desktop/src/environments/environment.ts",
                  "with": "apps/desktop/src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": false,
                "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "baseHref": "/frontend/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "ru": {
              "aot": true,
              "outputPath": "dist/desktop-ru",
              "i18nFile": "src/i18n/messages.ru.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "ru"
            },
            "en": {
              "localize": [
                "en"
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "desktop:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "desktop:build:production"
            },
            "en": {
              "browserTarget": "desktop:build:en"
            },
            "ru": {
              "browserTarget": "desktop:build:ru"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "desktop:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/desktop/tsconfig.app.json",
              "apps/desktop/tsconfig.spec.json",
              "apps/desktop/tsconfig.editor.json",
              "apps/desktop/.storybook/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/desktop/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/desktop/jest.config.js",
            "passWithNoTests": true
          }
        },
        "xliffmerge": {
          "builder": "@ngx-i18nsupport/tooling:xliffmerge",
          "options": {
            "xliffmergeOptions": {
              "i18nFormat": "xlf",
              "genDir": "apps/desktop/src/locale",
              "srcDir": "dist/locale",
              "defaultLanguage": "en",
              "languages": [
                "ru"
              ]
            }
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "apps/desktop/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/desktop",
            "config": {
              "configFolder": "apps/desktop/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "desktop-e2e": {
      "root": "apps/desktop-e2e",
      "sourceRoot": "apps/desktop-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/desktop-e2e/cypress.json",
            "tsConfig": "apps/desktop-e2e/tsconfig.e2e.json",
            "devServerTarget": "desktop:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "desktop:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/desktop-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/desktop-e2e/**/*"
            ]
          }
        }
      }
    },
    "logging": {
      "projectType": "library",
      "root": "libs/logging",
      "sourceRoot": "libs/logging/src",
      "prefix": "log",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/logging/tsconfig.lib.json",
            "project": "libs/logging/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/logging/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/logging/tsconfig.lib.json",
              "libs/logging/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/logging/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/logging/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    },
    "auth": {
      "projectType": "library",
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "prefix": "auth",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/auth/tsconfig.lib.json",
            "project": "libs/auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/auth/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/auth/tsconfig.lib.json",
              "libs/auth/tsconfig.spec.json",
              "libs/auth/.storybook/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/auth/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/auth/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4401,
            "config": {
              "configFolder": "libs/auth/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/auth",
            "config": {
              "configFolder": "libs/auth/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "desktop"
}
