{
    "name": "Network | MPLS | LSP Down (SNMP)",
    "$collection": "fm.eventclassificationrules",
    "uuid": "e2fa0214-79f2-4180-9e83-a3273c0db2bb",
    "description": "MPLS-MIB::mplsLspInfoDown",
    "event_class__name": "Network | MPLS | LSP Down",
    "preference": 1000,
    "patterns": [
        {
            "key_re": "^source$",
            "value_re": "^SNMP Trap$"
        },
        {
            "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
            "value_re": "^MPLS-MIB::mplsLspInfoDown$"
        },
        {
            "key_re": "^MPLS-MIB::mplsLspInfoName\\.\\.+$",
            "value_re": "^(?P<name>.+)$"
        },
        {
            "key_re": "^MPLS-MIB::mplsPathInfoName\\.\\.+$",
            "value_re": "^(?P<path>.+)$"
        }
    ]
}